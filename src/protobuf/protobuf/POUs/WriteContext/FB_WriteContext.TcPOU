    <Method Name="WriteInt32" Id="{cb424f8c-2286-402b-adff-21634a582503}">
      <Declaration><![CDATA[METHOD WriteInt32 : HRESULT
VAR_INPUT
    anyDint : ANY;
END_VAR
VAR
    nTmp   : DINT;
	retVal : UDINT;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[
retVal := memcpy(destAddr:= ADR(nTmp), srcAddr:= anyDint.pValue, n:= MIN(TO_UDINT(anyDint.diSize), SIZEOF(nTmp)));
IF retVal = 0 THEN
	WriteInt32:= E_HRESULTAdsErr.INVALIDDATA;
	RETURN;
END_IF

WriteInt32 := F_WriteInt32(THIS^.stCtx, nTmp);]]></ST>
      </Implementation>
    </Method>
