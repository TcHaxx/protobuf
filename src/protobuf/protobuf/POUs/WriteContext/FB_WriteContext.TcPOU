    <Method Name="WriteEnum" Id="{4fb9d4ab-4be6-42ee-a3bb-3f86dbf2061c}">
      <Declaration><![CDATA[METHOD WriteEnum : HRESULT
VAR_INPUT
    anyEnum : ANY;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF anyEnum.pValue = NULL OR anyEnum.TypeClass <> __SYSTEM.TYPE_CLASS.TYPE_ENUM THEN
	WriteEnum := E_HRESULTAdsErr.INVALIDDATA;
	RETURN;
END_IF

WriteEnum := F_WriteEnum(THIS^.stCtx, PDINT_TO_DINT(anyEnum.pValue));]]></ST>
      </Implementation>
    </Method>
